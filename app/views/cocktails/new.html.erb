<div class="container">
  <%= link_to "Back to all cocktails", cocktails_path %>

  <h1>New cocktail</h1>

  <%= simple_form_for(@cocktail) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
  <% end %>
</div>
