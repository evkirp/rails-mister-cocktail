<div class="container">
  <%= link_to "Back to all cocktails", cocktails_path %>

  <h1>New dose</h1>

  <%= form_for(@dose) do |f| %>
    <%= f.label :amount %>
    <%= f.text_field :amount %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.submit %>
  <% end %>
</div>
